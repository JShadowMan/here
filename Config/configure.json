{
  "environment": {
    "debug_mode": "on",
    "strict_router": "off",
    "auto_commit": "on"
  },
  "router": {
    "dashboard": "dashboard"
  },
  "database": [{
    "host": "mysql",
    "port": 3306,
    "username": "root",
    "password": "root",
    "database": "here",
    "charset": "utf8",
    "driver": "mysql"
  }],
  "cache": [{
    "name": "master",
    "host": "redis",
    "port": 6379,
    "driver": "redis"
  }],
  "request": {
    "policy": {
      "time_limit": 30,
      "count_limit": 30,
      "forbidden_time": 300
    }
  }
}
